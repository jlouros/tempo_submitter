{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "object-literal-sort-keys": false,
        "no-namespace": [true, "allow-declarations"],
        "interface-name": false,
        "no-console": false,
        "max-line-length": [true, 130]
    },
    "rulesDirectory": [],
    "prefer-const": true
}
